{
    "compilerOptions": {
      "outDir": "./dist",
      "target": "es5",
      "module": "commonjs",
      "noImplicitAny": true,
      "removeComments": true,
      "sourceMap": true,
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "alwaysStrict": true,
      "strictNullChecks": true,
      "jsx": "react",
      "jsxFactory": "h",
    },
    "include": [
      "./src/**/*.tsx",
      "./src/**/*.ts"
    ],
  }
  